datasource db {
    provider = "postgresql"
    url      = "postgresql://root:root@localhost:1999/proe"
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
}

model User {
    id       String @id @default(uuid())
    email    String @unique
    password String
}

model Contacts {
    id      String @id @default(uuid())
    name    String
    picture String
    device  String
    number  String @unique
    userID  String
}
